.outline{
  padding: 0 30rpx;
}
.outline .course-name{
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 32rpx;
  height: 134rpx;
  border-bottom: 1px solid #ECECEC;
}
.outline .no-border{
  border: 0;
}
.outline .course-name image{
  width: 24rpx;
  height: 14rpx;
}
.outline .course-left{
  display: flex;
}
.outline .course-name text:first-child{
  color: #D0D0D0;
  margin-right: 10rpx;
  font-weight: bold;
}
.outline .course-name text:nth-child(2){
  width: 520rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.pd50{
  padding-left: 50rpx;
}
.rotate-img{
  transform: rotate(180deg);
}
.outline .main-title{
  width: 520rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 32rpx;
}
.outline .sub-text{
  font-size: 26rpx;
  color: #666;
  margin-top: 10rpx;
}
.project-wrap{
  /* padding: 25rpx 32rpx; */
  background: #F5F5F5;
  margin: 0 0 32rpx;
}
.project-wrap .project{
  width: 100%;
  height: 123rpx;
  padding: 0 32rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: center;
  font-size: 26rpx;
  position: relative;
}
.project-wrap .project image{
  width: 20rpx;
  height: 20rpx;
  margin-right: 10rpx;
}
.project-wrap .ov{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.project-wrap .project-url{
  display: flex;
  align-items: center;
  font-size: 24rpx;
  margin-top: 10rpx;
}
.project-wrap .project-url view{
  color: #666;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.project-wrap .read-over{
  padding: 32rpx;
  position: relative;
}


.read-over .teacher{
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
}
.read-over .teacher-left{
  display: flex;
  align-items: center;
}
.read-over .teacher-left view{
  width: 96rpx;
  height: 32rpx;
  border: 1rpx solid #FD593F;
  border-radius: 3px;
  font-size: 24rpx;
  color: #FD593F;
  text-align: center;
  line-height: 32rpx;
  margin-left: 10rpx;
}
.read-over .teacher image{
  width: 56rpx;
  height: 56rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}
.read-over .teacher-right{
  font-size: 24rpx;
  color: #999;
}
.line::after{
  content: '';
  width: 567rpx;
  height: 1px;
  background: #ECECEC;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}
.read-over .evaluate{
  font-size: 28rpx;
  margin-top: 20rpx;
}